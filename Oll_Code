                                    if (TT == "Крести" & RR == "2")
                                    {
                                        pictureBox54.Location = new Point(492, 122);
                                    }
                                }
                            }
                        }
                    }

                    if (count2 <= 21)
                    {
                        label4.Text = count2.ToString();
                        label5.Text = "Вы проиграли! Дилер набрал больш очков.";
                        label5.Location = new Point(245, 202);
                    }
                    else
                    {
                        label4.Text = count2.ToString();
                        label5.Text = "Вы победили! У дилера перебор.";
                        label5.Location = new Point(245, 202);
                    }
                    
                }
                else
                {
                    if (count2 == count & count2 >= 17)
                    {
                        label4.Text = count2.ToString();
                        label5.Text = "Ничья! У вас поровну очков";
                        label5.Location = new Point(245, 202);
                    }
                    if (count2 > count & count2 <= 21)
                    {
                        label4.Text = count2.ToString();
                        label5.Text = "Вы проиграли! Дилер набрал больш очков.";
                        label5.Location = new Point(245, 202);
                    }
                }
            }

            //Проверка победы дилера
            if (count2 > count & count2 <= 21)
            {
                label4.Text = count2.ToString();
                label5.Text = "Вы проиграли! Дилер набрал больш очков.";
                label5.Location = new Point(245, 202);
            }
            if (count2 < count & count <= 21)
            {
                label4.Text = count2.ToString();
                label5.Text = "Вы победили! Дилер набрал меньше очков.";
                label5.Location = new Point(245, 202);
            }
            if(count2 == count)
            {
                label4.Text = count2.ToString();
                label5.Text = "Вы проиграли! Дилер набрал столько же очков.";
                label5.Location = new Point(245, 202);
            }

            //Вывод счета дилера
            label4.Text = count2.ToString();
        }
    }
}
