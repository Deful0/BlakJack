        //Кнопка хватит
        private void button6_Click(object sender, EventArgs e)
        {
            //Удаление закрытой карты дилера
            pictureBox3.Location = new Point(870, 0);

            //Возврат сообщения
            label5.Location = new Point(933, 0);
            label5.Text = "Win";

            int AA = random.Next(0, 13);
            int BB = random.Next(0, 4);
            string SS = koloda[AA];
            string ZZ = mast[BB];
            string fF = SS + " " + ZZ;

            int CC = random.Next(0, 13);
            int DD = random.Next(0, 4);
            string RR = koloda[CC];
            string TT = mast[DD];
            string gg = RR + " " + TT;

            //Вторая карта дилера
            int C = random.Next(0, 13);
            int D = random.Next(0, 4);
            string R = koloda[C];
            string T = mast[D];
            string g = R + " " + T;

            //Прибавляю к первой карты дилера вторую
            if (R == "J" || R == "Q" || R == "K" || R == "A")
            {
                if (R == "A")
                {
                    count2 += 11;
                    if (T == "Черви")
