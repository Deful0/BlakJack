        //Кнопка ещё
        private void button5_Click(object sender, EventArgs e)
        {
            //Возврат сообщения
            label5.Location = new Point(933, 0);
            label5.Text = "Win";

            int aa = random.Next(0, 13);
            int bb = random.Next(0, 4);
            string ss = koloda[aa];
            string zz = mast[bb];
            string FF = ss + " " + zz;

            //Ввод еще одной карты игрока
            if (ss == "J" || ss == "Q" || ss == "K" || ss == "A")
            {
                if (ss == "A")
                {
                    count += 11;
                    if (zz == "Черви")
                    {
                        pictureBox5.Location = new Point(479, 289);
                    }
                    else
                    {
                        if (zz == "Пики")
                        {
