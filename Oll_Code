        //Правила
        private void button1_Click(object sender, EventArgs e)
        {
            if (pictureBox2.Location == new Point(0, 0))
            {
                pictureBox2.Location = new Point(0, 600);
            }
            else
            {
                pictureBox2.Location = new Point(0, 0);
            }
                
        }

